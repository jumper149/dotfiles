# Set default values for all following accounts.
defaults
auth           on
tls            on

# gmail
account        gmail
host           smtp.gmail.com
port           587
from           felixspringer149@gmail.com
user           felixspringer149@gmail.com
passwordeval   "gpg --quiet --for-your-eyes-only --no-tty --decrypt ~/.password-store/mail/gmail.gpg"

# contact-felixspringer
account        contact-felixspringer
host           felixspringer.xyz
port           587
from           contact@felixspringer.xyz
user           contact@felixspringer.xyz
passwordeval   "gpg --quiet --for-your-eyes-only --no-tty --decrypt ~/.password-store/mail/contact-felixspringer.gpg"

# Set a default account
account default : gmail
